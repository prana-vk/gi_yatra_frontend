# Environment Variables
# Leave REACT_APP_API_URL unset to use the proxy configured in package.json (http://127.0.0.1:8000)

# Optional: Enable source maps in production (for debugging)
GENERATE_SOURCEMAP=true

# Optional: Disable ESLint warnings as errors
ESLINT_NO_DEV_ERRORS=true

# Optional: Browser to open during development
BROWSER=default

# Ensure dev server uses HTTP only (not HTTPS)
HTTPS=false