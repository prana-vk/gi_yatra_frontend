{
  "name": "giyatra-frontend",
  "version": "1.0.0",
  "description": "GI Yatra - React frontend for exploring Karnataka's geographical indication locations and planning trips",
  "keywords": [
    "react",
    "karnataka",
    "travel",
    "heritage",
    "trip-planning",
    "gi-locations"
  ],
  "author": "GI Yatra Team",
  "license": "MIT",
  "private": true,
  "homepage": ".",
  "proxy": "https://backend-k4x8.onrender.com",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.0",
    "framer-motion": "^11.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1"
  },
  
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 3000",
    "dev": "react-scripts start",
    "preview": "npm run build && npm run serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "serve": "^14.2.1"
  }
}
